{% extends 'partials/base.html.twig' %}

{% block content %}
<div class="post">
  <div class="entry-container">
    <div class="entry-content  single">
      <h1 class="entry-title single">{{ form.message }}</h1>
      {{ content }}
      {% if config.plugins.form.enabled %}
         {% include "forms/data.html.twig" %}
      {% endif  %}
      <div class="clear"></div>
    </div>
  </div>
</div>
{% endblock %}
